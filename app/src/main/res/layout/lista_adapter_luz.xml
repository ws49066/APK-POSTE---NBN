<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#111b31"
    android:orientation="vertical">


    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="142dp"
            android:layout_height="wrap_content"
            android:gravity="center">

            <TextView
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:padding="2dp"
                android:text="TIPO ILUMINAÇÃO"
                android:textAlignment="center"
                android:textColor="#00bfff"
                android:textSize="6pt"  />

        </LinearLayout>


        <LinearLayout
            android:layout_width="142dp"
            android:layout_height="wrap_content"
            android:gravity="center">

            <TextView
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:padding="2dp"
                android:text="TIPO DE LAMPADA"
                android:textAlignment="center"
                android:textColor="#00bfff"
                android:textSize="6pt"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="142dp"
            android:layout_height="wrap_content"
            android:gravity="center">

            <TextView
                android:layout_width="111dp"
                android:layout_height="wrap_content"
                android:padding="2dp"
                android:text="POTENCIA"
                android:textAlignment="center"
                android:textColor="#00bfff"
                android:textSize="6pt" />
        </LinearLayout>
    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="142dp"
            android:layout_height="wrap_content"
            android:gravity="center">

            <TextView
                android:id="@+id/tipoiluminacao"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:padding="2dp"
                android:text="TIPO ILUMINAÇÃO"
                android:textAlignment="center"
                android:textColor="#fff"
                android:textSize="6pt" />

        </LinearLayout>


        <LinearLayout
            android:layout_width="142dp"
            android:layout_height="wrap_content"
            android:gravity="center">

            <TextView
                android:id="@+id/tipo_lampada_texto"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:padding="2dp"
                android:text="Aquariquara"
                android:textAlignment="center"
                android:textColor="#fff"
                android:textSize="6pt" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="142dp"
            android:layout_height="wrap_content"
            android:gravity="center">

            <TextView
                android:id="@+id/potencia_texto"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:padding="2dp"
                android:text="16WT"
                android:textAlignment="center"
                android:textColor="#fff"
                android:textSize="6pt" />
        </LinearLayout>
    </LinearLayout>





</LinearLayout>